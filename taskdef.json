{
    "executionRoleArn": "arn:aws:iam::532783812193:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "nginx01",
            "image": "532783812193.dkr.ecr.ap-northeast-1.amazonaws.com/ys-nginx:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        },
        {
            "name": "tomcat01",
            "image": "532783812193.dkr.ecr.ap-northeast-1.amazonaws.com/ys-tomcat:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8090,
                    "protocol": "tcp",
                    "containerPort": 8090
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}
